{% extends 'base.html.twig' %}

{% block title %}Note{% endblock %}

{% block body %}
    <h1>Note</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ note.id }}</td>
            </tr>
            <tr>
                <th>LibelleNote</th>
                <td>{{ note.libelleNote }}</td>
            </tr>
            <tr>
                <th>DateNote</th>
                <td>{{ note.dateNote ? note.dateNote|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Commentaire</th>
                <td>{{ note.commentaire }}</td>
            </tr>
            <tr>
                <th>Note</th>
                <td>{{ note.note }}</td>
            </tr>
            <tr>
                <th>Coefficient</th>
                <td>{{ note.coefficient }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('note_index') }}">back to list</a>

    <a href="{{ path('note_edit', {'id': note.id}) }}">edit</a>

    {{ include('note/_delete_form.html.twig') }}
{% endblock %}
